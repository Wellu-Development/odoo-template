<odoo>
    <record id="nena_person_type_view_list" model="ir.ui.view">
        <field name="name">nena.person.type.view.list</field>
        <field name="model">nena.person.type</field>
        <field name="arch" type="xml">
            <list string="Type of Person" editable="bottom">
                <field name="name"/>
            </list>
        </field>
    </record>

    <record id="nena_person_type_action" model="ir.actions.act_window">
        <field name="name">Type of Person</field>
        <field name="res_model">nena.person.type</field>
        <field name="view_mode">list</field>
    </record>

    <menuitem
        id="nena_person_type_menu"
        name="Type of Person"
        action="nena_person_type_action"
        parent="nena_documents_menu"
        sequence="15"/>  
</odoo>