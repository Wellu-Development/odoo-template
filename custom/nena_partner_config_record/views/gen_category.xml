<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_nena_gen_category_list" model="ir.ui.view">
        <field name="name">nena.gen.category.view.tree</field>
        <field name="model">nena.gen.category</field>
        <field name="type">list</field>
        <field name="arch" type="xml">
            <list string="Categorias" editable="bottom">
                <field name="code"/>
                <field name="category"/>
                <field name="active" widget="boolean_toggle"/>
                <field name="entity" widget="boolean_toggle"/>
                <field name="operational" widget="boolean_toggle"/>
            </list>
        </field>
    </record>

    <record id="nena_gen_category_action" model="ir.actions.act_window">
        <field name="name">Categorias</field>
        <field name="res_model">nena.gen.category</field>
        <field name="view_mode">list</field>
    </record>
</odoo>