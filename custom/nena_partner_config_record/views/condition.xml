<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_nena_condition_list" model="ir.ui.view">
        <field name="name">nena.condition.view.tree</field>
        <field name="model">nena.condition</field>
        <field name="type">list</field>
        <field name="arch" type="xml">
            <list string="Condiciones" editable="bottom">
                <field name="code"/>
                <field name="description"/>
                <field name="gen_management_id" domain="[('code', 'in', ['COB', 'REG', 'VTA', 'WMS'])]"
                        options="{'no_quick_create':True,'no_create':True,'no_create_edit':True,'no_open': True}"/>
                <field name="ref" />
            </list>
        </field>
    </record>

    <record id="nena_condition_action" model="ir.actions.act_window">
        <field name="name">Condiciones</field>
        <field name="res_model">nena.condition</field>
        <field name="view_mode">list</field>
    </record>
</odoo>